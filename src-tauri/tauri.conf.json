{
  "$schema": "https://schema.tauri.app/config/2.0.json",
  "productName": "StellarS3",
  "version": "1.0.0",
  "identifier": "com.stellars3.app",
  "app": {
    "windows": [
      {
        "title": "StellarS3",
        "width": 1200,
        "height": 800,
        "resizable": true,
        "fullscreen": false,
        "decorations": true,
        "transparent": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../dist"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "identifier": "com.stellars3.app",
    "icon": [
      "icons/icon.ico",
      "icons/icon.png"
    ]
  },
  "plugins": {}
}